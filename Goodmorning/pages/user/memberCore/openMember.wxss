/* pages/user/memberCore/openMember.wxss */
page{
  position: relative;
  background-color: #fff;
}

.line{
   width: 100%;
  height: 12rpx;
  background-color: #efeff4;
}
/* 个人信息框 */
.userbox{
  width: 100%;
  height: 170rpx;
  overflow: hidden;
  border:1px solid #bbbbbb; 
}

.userimg{
 width:130rpx;
height:128rpx;
border-radius:10rpx;
float:left;
margin-left:30rpx;
margin-top:22rpx;
margin-right:24rpx;

}

.userinfo {
  float: left;
  font-size: 12px;
  color: #100f10;
}

.name{
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}

.hybox{
  width: 100%;
  height: 300rpx;
}

.openhy{
  width:100%;
  height:80rpx;
  padding-left: 34rpx;
  line-height: 80rpx;
  border-bottom:1px solid #bbbbbb; 
}

.yearbox{
  width:230rpx;
  height:158rpx;
  border:1px solid #bbbbbb;
  margin:0 auto;
  margin-top: 30rpx;
  border-radius: 10px;
}

.yearbox .year{
display: block;
 font-size: 16px;
 margin-top:12rpx;
 margin-left: 124rpx;
 margin-bottom: 8rpx;
}

.yearbox .day{
  display: block;
 font-size: 20px;
 color:#e74240;
 margin-left: 62rpx;
  margin-bottom: 5rpx;
}

.yearbox .time{
  display: block;
  /* margin-left: 12rpx; */
  margin-left:18rpx;
 font-size: 12px;
 color:#101010;
}

.stylebox{
  width: 100%;
  height: 80rpx;
line-height: 80rpx;
float: left;
overflow: hidden;
border-bottom: 1px solid #e6e6e6;
}

.stylebox1{
  width: 100%;
  height: 80rpx;
line-height: 80rpx;
float: left;
overflow: hidden;
}

.style{
   line-height: 80rpx;
  margin-left: 34rpx;
}

.style1{
  float: left;
  width: 300rpx;height: 30rpx;
   line-height: 80rpx;
  margin-left: 34rpx;
}

.jtimg{
  float: right;
  display: block;
  margin-top: 26rpx;margin-right: 48rpx;
  width: 20rpx;height: 30rpx;
}

.jtimg1{
  float: right;
  display: block;
  margin-top: 26rpx;margin-right: 48rpx;
  width: 24rpx;height: 30rpx;
}


.wxbox{
   width: 100%;
  height: 80rpx;
  overflow: hidden;
  background-color: #eeeeee;
}

.wxbox1{
   width: 100%;
  height: 80rpx;
  overflow: hidden;
}

.wx{
  float: left;
  display: block;
  width: 58rpx;
  height: 52rpx;
  margin:16rpx 24rpx 0 36rpx;
}

.wxy{
  float: right;
   width: 30rpx;
  height: 40rpx;
  margin-top: 16rpx;
}

.jsbox{
  width: 100%;
  height: 80rpx;
  display: flex;
  display: -webkit-flex;
    position: fixed;
   left: 0px;
   bottom: 0px;
}

.jsbox .left{
  flex: 4;
  -webkit-flex:4;
  height: 80rpx;

}

 .left text{
   line-height: 80rpx;
   margin-left: 22rpx;

}

.jsbox .right{
  flex: 1;
  -webkit-flex:1;
  height: 80rpx;
   background-color: #d25b4a;
   color: #fff;
}

.right text{
  
  margin-left: 20rpx;
  line-height: 80rpx;
}

.sb-ts{
  text-align: right;
  color: #e64340;
}
.s2b-ts{
  overflow: hidden;
  color: #e64340;
  margin-right: 4%;
}
.s2b-ts view{
  float: right;
  width: 20rpx;
  height: 20rpx;
  border: #e64340 solid 1px;
  margin: 10rpx 10rpx 0 0;
}
.s2b-ts text{
  float: right;
}

.radio-group{
  float: right;
  margin: 16rpx 56rpx 0 0;
}

.allbox{
  float: right;
  width: 190rpx;
  height: 76rpx;
  overflow: hidden;
   margin-right: 30rpx;
   /* border: 1px solid red; */
}

 .allbox text{
    font-size: 20px;
    height: 76rpx;
    line-height: 76rpx;
    display: inline-block;
    margin-top: 10rpx;
} 

.allbox image{
  display: inline-block;
  margin-top: 20rpx;
  width: 50rpx;height: 50rpx;
}

.allbox image:first-child{
  float: left;
  margin-left: 10rpx;margin-right: 20rpx;
}

.allbox image:last-child{
  float: right;
  margin-left: 10rpx;
}

.checkbox{
  margin:20rpx 0;
  margin-left: 336rpx;
  width:390rpx;
  display: block;
  color: #e84240;
}

.checkbox:first-child{
  margin-top: 40rpx;

}

.check{
  width: 20rpx;height: 20rpx;
}
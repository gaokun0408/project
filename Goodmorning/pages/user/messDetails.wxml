<!--pages/user/messDetails.wxml-->
<view class="line"></view>
<view class="messbox" wx:for='{{messList}}' wx:key='id'>
  <view class="titbox">
    <text class="date">{{item.date}}</text>
    <text class="name" bindtap="goMess">{{item.time}}</text>
  </view>
  <view class="mess">{{item.message}}</view>
</view>

